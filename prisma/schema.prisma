// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("SQLITE_URL")
}

model Ticket {
  id       Int    @id @default(autoincrement())
  name     String
  price    Float
  quantity Int
}

model Event {
  id   Int      @id @default(autoincrement())
  name String   @unique
  date DateTime
}
